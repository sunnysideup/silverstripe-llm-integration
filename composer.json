{
    "name": "sunnysideup/llm-integration",
    "description": "EXPERIMENTAL ONLY: converse with an LLM to get information and let it do stuff in your CMS",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "llm",
        "ai",
        "dangerous",
        "interactive",
        "data management"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^5.0",
        "silverstripe/admin": "^2.0"
    },
    "require-dev": {
        "sunnysideup/sswebpack_engine_only": "dev-master",
        "silverleague/ideannotator": "*",
        "phpunit/phpunit": "^9.6",
        "squizlabs/php_codesniffer": "^3.7",
        "silverstripe/standards": "^1",
        "phpstan/extension-installer": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Sunnysideup\\LlmIntegration\\": "src/"
        }
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "bin/sake-npm-build-llm-integration.sh",
        "bin/sake-npm-watch-llm-integration.sh"
    ],
    "authors": [
        {
            "name": "Nicolaas Thiemen Francken",
            "email": "modules@sunnysideup.co.nz",
            "role": "lead developer"
        }
    ]
}